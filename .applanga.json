{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./JSON/en/en.json",
          "file_format": "react_simple_json",
          "tag": "JSON",
          "language": "en"
        },
        {
          "path": "./xliff/en/en.xliff",
          "file_format": "xliff",
          "tag": "Xliff",
          "language": "en"
        },
        {
          "path": "./Excel/en/en.xlsx",
          "file_format": "xls",
          "tag": "Excel",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "excludeHeaderRow": true,
          "language": "en"
        },
        {
          "path": "./Android-XML/en/en.xml",
          "file_format": "android_xml",
          "tag": "Android XML",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./JSON/<language>/<language>.json",
          "file_format": "react_simple_json",
          "tag": "JSON",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./xliff/<language>/<language>.xliff",
          "file_format": "xliff",
          "tag": "Xliff",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./Excel/<language>/<language>.xlsx",
          "file_format": "xls",
          "tag": "Excel",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "columnDescription": {
            "KEY": 0,
            "<language>": 1
          },
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./Android-XML/<language>/<language>.xml",
          "file_format": "android_xml",
          "tag": "Android XML",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}